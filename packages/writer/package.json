{
  "name": "@flipbook/writer",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
